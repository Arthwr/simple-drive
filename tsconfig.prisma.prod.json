{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "rootDir": "./prisma",
    "outDir": "./dist/prisma",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["prisma/**/*"],
  "exclude": ["node_modules"]
}